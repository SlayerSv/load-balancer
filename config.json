{
    "port": "8080",
    "load_balancer": {
        "backend_urls": ["localhost:8081", "localhost:8082"],
        "health_check_interval": 10,
        "worker_count": 2,
        "health_check_timeout": 1
    },
    "rate_limiter": {
        "add_tokens_interval": 1,
        "save_state_interval": 10,
        "remove_stale_interval": 300,
        "cache": {
            "time_to_live": 300
        }
    }
}